<Project Sdk="Microsoft.NET.Sdk.Razor">
  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <Version>1.0</Version>
    <AssemblyName>Senparc.Xncf.AuditLog</AssemblyName>
    <RootNamespace>Senparc.Xncf.AuditLog</RootNamespace>
    <GeneratePackageOnBuild Condition=" '$(Configuration)' == 'Release' ">true</GeneratePackageOnBuild>
    <Description>与安全相关的按照时间顺序的记录，记录集或者记录源</Description>
    <Copyright>Senparc</Copyright>
    <PackageTags>Senparc,NeuCharFramework,NCF,Senparc.Xncf.AuditLog</PackageTags>
    <Authors>Senparc</Authors>
    <Owners>Senparc</Owners>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <Title>审计日志</Title>
    <!--<ProjectUrl> https://github.com/NeuCharFramework/NCF</ProjectUrl>
    <PackageProjectUrl>https://github.com/NeuCharFramework/NcfPackageSources</PackageProjectUrl>-->
    <PackageIcon>icon.jpg</PackageIcon>
    <PackageReleaseNotes>
			v1.0 创世
</PackageReleaseNotes>
    <RepositoryUrl>https://github.com/NeuCharFramework/NcfPackageSources</RepositoryUrl>
    <Configurations>Debug;Release;Test;TemplateRelease</Configurations>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
    <OutputPath>..\..\..\BuildOutPut</OutputPath>
  </PropertyGroup>

	
  <ItemGroup>
    <PackageReference Include="Dapper" Version="2.0.123" />
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="6.0.8" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="6.0.8" />
    <PackageReference Include="Senparc.CO2NET.WebApi" Version="1.1.2" />
    <!--<PackageReference Include="Microsoft.AspNetCore.Mvc.Versioning" Version="5.0.0" />-->
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Authentication.Cookies" Version="2.2.0" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Senparc.Ncf.DatabasePlant" Version="0.11.4.1-beta7" />
    <PackageReference Include="Senparc.Ncf.Service" Version="0.11.3-beta7" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Senparc.Ncf.XncfBase" Version="0.11.3-beta7" />
    <PackageReference Include="Senparc.Ncf.AreaBase" Version="0.11.2-beta7" />
    <None Include="App_Data\SenparcTraceLog\*" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Authentication.Cookies" Version="2.2.0" />
    <PackageReference Include="Microsoft.AspNetCore.Mvc" Version="2.2.0" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.Design" Version="5.0.1" />
    <!-- 为解决 VS 2019 v16.8 的一个bug，需要添加下方引用 -->
    <PackageReference Include="Microsoft.NET.Sdk.Razor" Version="3.1.10" />
    <PackageReference Include="System.Data.SqlClient" Version="4.8.3" />
  </ItemGroup>
 <ItemGroup>
      <None Include="..\Senparc.Xncf.AuditLog\icon.jpg" Pack="true" PackagePath="" />
  </ItemGroup>
 <ItemGroup>
   <Folder Include="Domain\Migrations\" />
   <Folder Include="Migrations\Migrations.SqlServer\" />
   <Folder Include="Migrations\Migrations\SqlServer\" />
 </ItemGroup>

</Project>