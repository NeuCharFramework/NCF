	</ItemGroup>
	<ItemGroup Condition="'$(Configuration)'=='ModifyPublish'">
		<Content Update="App_Data\**\*.*">
			<CopyToPublishDirectory>Never</CopyToPublishDirectory>
		</Content>
		<Content Update="web.config">
			<CopyToPublishDirectory>Never</CopyToPublishDirectory>
		</Content>
		<Content Update="appsettings*.json">
			<CopyToPublishDirectory>Never</CopyToPublishDirectory>
		</Content>
	</ItemGroup>
